        {% else %}
        <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1095">
        {% endif %}
          <a href="/infopage1">Info Page 1</a>
        </li>
        {% if request.url_rule.endpoint == "main_bp.infopage2" %}
