        {% else %}
        <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1098">
        {% endif %}
          <a href="/infopage2">Info Page 2</a>
        </li>
        {% if request.url_rule.endpoint == "main_bp.contact" %}
        <li class="menu-item menu-item-type-post_type menu-item-object-page current-menu-item page_item page-item-1216 current_page_item menu-item-1222">
        {% else %}
        <li class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1222">
        {% endif %}
          <a href="/contact">Contact</a>
        </li>
      </ul>
      <ul class="navbar-right">
        {% if session.usertype == "Admins"%}
        {% if request.url_rule.endpoint == "main_bp.ViewOrAddUsers" or  request.url_rule.endpoint == "main_bp.admininfo" %}
        <li class="dropdown current-menu-item">
        {% else %}
        <li class="dropdown">
        {% endif %}
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Admin Menu<span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li><a href="/admin/users">Users</a></li>
            <li><a href="/admin/information">Admin info</a></li>
          </ul>
        </li>
        {% endif %}
        {% if session.logged_in %}
        {% if request.url_rule.endpoint == "main_bp.change_pwd" or  request.url_rule.endpoint == "main_bp.account" %}
        <li class="dropdown current-menu-item">
        {% else %}
        <li class="dropdown">
        {% endif %}
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Account<span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li class="dropdown-header">Logged in as <b>{{session.username}}</b></li>
            <li><a href=/change-pwd>Change Password</a> </li>
            <li><a href=/account/{{session.username}}>Account</a> </li>
            <li><a href=/logout>Logout </a> </li>
          </ul>
        </li>
        {% else %}
        {% if request.url_rule.endpoint == "main_bp.login" %}
        <li class="nav navbar-nav current-menu-item">
        {% else %}
        <li class="nav navbar-nav">
        {% endif %}
          <a href="/login"><span class="glyphicon glyphicon-log-in"> Login</a>
        </li>
        {% endif %}
      </ul>

    </div>
  </div>
</nav>
